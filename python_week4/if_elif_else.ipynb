   "source": [
    "#Convert number to letter grade\n",
    "score = 100\n",
    "if score >= 90:\n",
    "    letter = \"A\"\n",
    "elif score >= 80:\n",
    "    letter = \"B\"\n",
    "elif score >= 70:\n",
    "    letter = \"C\"\n",
    "elif score >= 60:\n",
    "    letter = \"D\"\n",
    "else:\n",
    "    letter = \"F\"\n",
    "print(f\"Score {score} = Grade {letter}\")"
   ]
